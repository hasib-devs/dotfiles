export ZSH="$HOME/.oh-my-zsh"

ZSH_THEME="frontcube"

plugins=(git fzf-tab zsh-autosuggestions zsh-syntax-highlighting)

source $ZSH/oh-my-zsh.sh

alias v="nvim"
alias c="clear"
alias pn="pnpm"
alias pni="pnpm install"
alias pna="pnpm add"
alias pnad="pnpm add -D"
alias pnd="pnpm dev"
alias pnb="pnpm build"
alias D="cd ~/Documents"
alias P="cd ~/Documents/Probaho"
alias R="cd ~/Documents/Rainbow"
alias RC="cd ~/Documents/Rainbow/Casino"
alias RCA="cd ~/Documents/Rainbow/Casino/crypto-casino-admin"
alias RCI="cd ~/Documents/Rainbow/Casino/crypto-casino-api"